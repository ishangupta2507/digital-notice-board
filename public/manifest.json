{
  "name": "Digital Notice Board",
  "description": "Open your go-to tabs in one click",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Digital Notice Board",
    "default_icon": {
      "16": "logo.png",
      "128": "logo.png"
    }
  },
  "permissions": [
    "tabs"
  ]
}
